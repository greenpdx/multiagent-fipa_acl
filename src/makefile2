# file: makefile2
#
# version: 1.0


objects = ../bin/ACLMessage.o ../bin/AgentAID.o ../bin/UserdefParam.o ../bin/ACLMessageOutputParser.o
headers = ACLMessage.h AgentAID.h UserdefParam.h ACLMessageOutputParser.h 

../bin : ../bin/test.o $(objects)
	g++ ../bin/test.o ../../message-parser/bin/message_parser.o ../../message-parser/bin/grammar_bitefficient.o $(objects) -o ../bin/Project
	

../bin/test.o : test.cpp ../../message-parser/src/message_parser.h $(headers)
	g++ -c test.cpp -o ../bin/test.o

../bin/ACLMessage.o : ACLMessage.cpp ACLMessage.h $(headers)
	g++ -c ACLMessage.cpp -o ../bin/ACLMessage.o

../bin/AgentAID.o : AgentAID.cpp AgentAID.h
	g++ -c AgentAID.cpp -o ../bin/AgentAID.o

../bin/UserdefParam.o : UserdefParam.cpp UserdefParam.h $(headers)
	g++ -c UserdefParam.cpp -o ../bin/UserdefParam.o

../bin/ACLMessageOutputParser.o : ACLMessageOutputParser.cpp ACLMessageOutputParser.h $(headers)
	g++ -c ACLMessageOutputParser.cpp -o ../bin/ACLMessageOutputParser.o



clean :
	rm -rf ../bin/test.o ../../message-parser.o $(objects) ../bin
